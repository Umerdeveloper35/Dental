    });
    if (benefitType === "employer") {
        textfield.parents('.benefits').find(".matching-dental .member-phone").html("<a href='tel:+1" + stateObject.outOfStateNumber + "'>" + formatPhoneNumber(stateObject.outOfStateNumber.replace(/-/g, "")) + "</a>");
