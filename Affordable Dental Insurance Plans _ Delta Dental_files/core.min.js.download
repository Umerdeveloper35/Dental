    ////////////////////////////////////////////////////////////////////////////////////////////////
    // Temporarily disable tabbing while modal is opening
    ////////////////////////////////////////////////////////////////////////////////////////////////
    $('[data-toggle="modal"]').on({
        'click' : function (e)
        {
            $(this).blur();

            tabDisabled = true;
            
            rememberFocus( this );
        }
    });
    ////////////////////////////////////////////////////////////////////////////////////////////////


    ////////////////////////////////////////////////////////////////////////////////////////////////
