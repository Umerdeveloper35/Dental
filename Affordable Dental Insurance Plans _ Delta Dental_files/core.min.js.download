    if(jsonRes && jsonRes.stateDetails.length > 0 && typeof jsonRes == "object"){
        $.each(jsonRes.stateDetails,function(key,state){
            states.push(state.stateName);
        });
    }
    headerDualMcJs(data,className,states);
